<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.yurinero.hashana.PasswordGenerator">
   <children>
      <TextField fx:id="passwordOutput" editable="false" layoutX="212.0" layoutY="98.0" />
      <Button fx:id="generateButton" layoutX="266.0" layoutY="202.0" mnemonicParsing="false" onAction="#handleGenerateButton" text="Generate" />
      <CheckBox fx:id="letterCheckbox" layoutX="198.0" layoutY="175.0" mnemonicParsing="false" text="Letters" />
      <CheckBox fx:id="symbolCheckbox" layoutX="267.0" layoutY="175.0" mnemonicParsing="false" text="Symbols" />
      <CheckBox fx:id="numberCheckbox" layoutX="348.0" layoutY="175.0" mnemonicParsing="false" text="Numbers" />
      <Separator layoutX="201.0" layoutY="165.0" prefWidth="200.0" />
      <Slider fx:id="lengthSlider" layoutX="208.0" layoutY="139.0" />
      <TextField fx:id="lengthDisplay" layoutX="361.0" layoutY="134.0" prefHeight="25.0" prefWidth="44.0" />
      <Button fx:id="copyButton" layoutX="367.0" layoutY="98.0" mnemonicParsing="false" onAction="#handleCopyButton" text="Copy" />
   </children>
</AnchorPane>
